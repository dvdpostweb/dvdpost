<% logger.debug("@@@#{params.count}") %>
<% if params.count <= 5 %>
   $('#all').html("<%= escape_javascript(render(:partial => 'products/index/hp', :formats => [:html])) %>");
  $('.loading_bar').hide();
  endscroll()
  load_form()
  $('.ca-container').contentcarousel({sliderSpeed: 500,sliderEasing: 'easeOutExpo',itemSpeed: 500,itemEasing: 'easeOutExpo', scroll: 6});
<% else %>

  <% if !params[:page].nil? && cookies[:endless] != 'deactive' %>
    $('#products').append("<%= escape_javascript(render(:partial => cookies[:display] == 'jacket' ? 'products/index/movie_details_jacket' : 'products/index/movie_detail', :collection => @products, :locals => {:target => @target, :jacket_mode => @jacket_mode, :rating_color => @rating_color}, :formats => [:html])) %>");  
    <% if (params[:type] == 'directors' && @directors.next_page) || (params[:type] == 'actors' && @actors.next_page) || ((params[:type] == 'products' || params[:type] == nil) && @products.next_page) %>
      $('#pagination_content').html('<%= escape_javascript(render(:partial => 'products/index/pagination', :locals => {:item => @products})) %>');
    <% else %>
      $('#pagination_content').html('');
    <% end %>
  <% else %>
  <% logger.debug("@@@all ") %>

    $('#all').html("<%= escape_javascript(render(:partial => 'products/index/all', :formats => [:html])) %>");
    load_form()
  <% end %>
  $('.loading_bar').hide();
  endscroll()
<% end %>
ga('send', 'pageview');
