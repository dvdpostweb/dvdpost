<%= render :partial => 'layouts/meta' %>
<%= stylesheet_link_tag 'style.css' %>
<%= stylesheet_link_tag 'facebox.css' %>
<%= stylesheet_link_tag 'messages.css' %>
<%= stylesheet_link_tag 'smoothness/jquery-ui-1.8.custom.css' %>
<%= stylesheet_link_tag 'jquery_custom.css' %>
<!--%= stylesheet_link_tag 'print.css', :media => 'print' %-->
<%= javascript_include_tag :defaults %>
<%= javascript_include_tag 'messages' %>
<%= javascript_include_tag 'jquery.ba-url.min' %>
<%= javascript_include_tag 'jquery-ui-1.8.custom.min' %>
<%= javascript_include_tag 'jquery.query-2.1.7' %>
<%= javascript_include_tag 'facebox' %>
<%= javascript_include_tag 'json2' %>
<!--[if IE 6]>
<%= javascript_include_tag 'DD_belatedPNG' %>
<%= javascript_include_tag 'ie6-dd-decl' %>
<![endif]-->
</head>
<body id="<%= @body_id || '' %>">
<div id="wrap">
  <!--   ==============   HEADER   ==============   -->
  <%= render :partial => 'layouts/header' %>
  <!--   ==============   END HEADER   ==============   -->
  <div class="container clearfix" >
    <%= flash[:notice] %>
    <%= yield :layout %>
  </div>
</div>
<!--   ==============   FOOTER   ==============   -->
<%= render :partial => 'layouts/footer' %>
<!--   ==============   END FOOTER   ==============   -->
<%= hide_wishlist_if_seen %>
</body>
</html>
