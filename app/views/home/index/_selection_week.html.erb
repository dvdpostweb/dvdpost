<!-- Selection de la semaine -->
<div id="selection-week-wrap">
  <div class="ttl maven"><%= t '.title' %></div>
  <div id="selection-tabcontent-wrap">
    <%= render :partial => '/home/index/menu_selection', :locals => {:selection_week => selection_week} %>
    <div class="tab-content-selection clearfix" id="selection-tab1">
      <!-- Carousel -->
      <div class="carousel-wrap">
        <ul class="slides">
          
          <%= render :partial => 'home/index/product_selection_week', :collection => selection_week %>
        </ul>
        <div class="pagination">
          <span class="hide_arrow">
            <%= will_paginate selection_week, :previous_label => '', :next_label => '', :param_name => :selection_page, :container => false %>
          </span>
        </div>
        <div class="pagination2">
          <%= will_paginate selection_week, :previous_label => '', :next_label => '', :page_links => false, :param_name => :selection_page, :container => false %>
        </div> 
      
        
      </div>
      <!-- Quâ€™ est ce que la vod ? -->
      <div id="vod-banner">
        <div class="caption">
          <h3 class="maven"><%= t '.vod' %></h3>
          <a href="<%= streaming_products_faq_path() %>"><%= t '.more' %></a> </div>
        <a href="<%= streaming_products_faq_path() %>"><img src="images/vod-banner.jpg" alt="<%= t '.vod' %>" /></a> </div>
    </div>
  </div>
</div>

